{
    "version": 2,,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "//(.*)",
        "dest": "nodeserver/index.js"
      },
      {
        "src": "/nodeserver",
        "dest": "index.html"
      }
    ]
  }
  